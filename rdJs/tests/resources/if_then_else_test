var fe;

func capitaocaverna(caveeeerna) {
  var forca;
  fe = false;
  func local() { forca = 1; }
  caveeeerna = false;
  return caveeeerna != true;
}

if (planetacomproblemas) {
  func capitaoplaneta(terra, fogo, ar, agua) {
    var coracao;
    coracao = terra + fogo + ar + agua;
    capitaocaverna(coracao);
  }
} else {
  func local() {
    var forca;
  }
}
