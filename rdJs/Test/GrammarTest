var foco, forca, fe;
fe = foco + 3;
forca = -(foco*(fe - (foco + fe)));
for (1; 1) {
  if (fe >= 55) {
    foco = true;
  }
}
if (planetaindomal) {
  func capitaoplaneta(terra, fogo, ar, agua) {
    var coracao;
    coracao = terra + fogo + ar + agua;
    capitaocaverna(coracao);
  }
} else {
  func capitaocaverna(caveeeerna) {
    caveeeerna = false;
    return caveeerna != true;
  }
}
